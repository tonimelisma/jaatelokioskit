{
  "expo": {
    "name": "Jäätelökioskit",
    "description": "Suomen jäätelökioskit kartalla",
    "owner": "tonimelisma",
    "sdkVersion": "37.0.0",
    "privacy": "public",
    "githubUrl": "https://github.com/tonimelisma/jaatelokioskit",
    "slug": "jaatelokioskit",
    "scheme": "jaatelokioskit",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#281E78"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "fi.tonimelisma.jaatelokioskit",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app will show your location and centralize the map for ease of use"
      }
    },
    "android": {
      "package": "fi.tonimelisma.jaatelokioskit",
      "versionCode": 1,
      "permissions": ["ACCESS_FINE_LOCATION"]
    }
  }
}
